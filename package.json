
{
  "name": "projnotes-ie22",
  "version": "0.0.0",
  "private": true,
  "scripts":{
    "start": "cross-env NODE_ENV=production node ./dist/bin/www",
    "b:w": "cross-env NODE_ENV=development babel-watch ./server/bin/www",
    "cli:ser": "cross-env NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "cle:ser": "rimraf dist",
    "cle:cli": "rimraf public/javascripts/bundle.js && rimraf public/stylesheets/styles.css",
    "bu:ser": "npm run clean && babel ./server --out-dir dist --copy-files",
    "bu:cli": "npm run cle:cli && webpack --config ./webpack.config.js --mode production"
  },

  "dependencies":{
    "app-root-path": "^3.0.0",
    "cookie-parser": "~1.4.4",
