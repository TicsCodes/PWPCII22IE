    "express-handlebars": "^6.0.3",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "rimraf": "^3.0.2",
    "winston": "^3.6.0"
  },

  "devDependencies":{
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5", 
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-minify": "^0.5.1",
    "babel-watch": "^7.7.0",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.0",
    "eslint": "^8.2.0",
